{
  "name": "voyager-ui",
  "private": true,
  "version": "1.0.0",
  "description": "UI interface for new Synopsys Checkin Manager System",
  "main": "index.tsx",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "author": {
    "name": "Samuel Valdes Gutierrez",
    "email": "valdesgutierrez@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigSamu/react_mui_ts_vite_boilerplate"
  },
  "bugs": {
    "url": "https://github.com/BigSamu/react_mui_ts_vite_boilerplate/issues"
  },
  "homepage": "https://github.com/BigSamu/react_mui_ts_vite_boilerplate#readme",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "mock_server": "json-server --watch db.json --port 8008",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write . --config ./.prettierrc.cjs",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "eslint"
    ],
    "*/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "json-server": "^1.0.0-beta.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.23.1",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  }
}